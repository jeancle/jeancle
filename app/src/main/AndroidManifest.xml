<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jeancle.cheap">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="false"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:launchMode="singleTask" />
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.alibaba.app.appkey"
            android:value="23772830" />
        <meta-data
            android:name="com.alibaba.app.pid"
            android:value="mm_42729943_12098598_72122685" />

        <activity android:name=".activity.WelcomeActivity" />
        <activity android:name=".activity.login.LoginActivity" />
        <activity android:name=".activity.login.RegisterActivity" />
        <activity android:name=".activity.login.FindPwdActivity" />
        <activity android:name=".activity.SettingActivity" />
        <activity android:name=".activity.GoodsDetailActivity"></activity>
    </application>

</manifest>